<org.eventb.core.axiom name="axm5" org.eventb.core.comment="The set of value of fighting:&#10;-1 : error&#10;0  : fair&#10;1  : hand 1 win 2&#10;2  : hand 2 win 1 " org.eventb.core.label="result_D" org.eventb.core.predicate="result = {−1,0,1,2}"/>
<org.eventb.core.axiom name="b" org.eventb.core.comment="Check a set cards is or is not empty?" org.eventb.core.label="isEmpty_D" org.eventb.core.predicate="isEmpty∈ℙ(SetID)→BOOL"/>
<org.eventb.core.axiom name="a" org.eventb.core.comment="If the cardinate of a set cards a is 0, then it is empty." org.eventb.core.label="isEmpty_F" org.eventb.core.predicate="∀a·a∈ℙ(SetID)∧card(a)=0 &#10;⇒ isEmpty(a)=TRUE"/>
<org.eventb.core.axiom name="\" org.eventb.core.comment="Check the set of cards has two cards same id or not?" org.eventb.core.label="isUnique_D" org.eventb.core.predicate="isUnique ∈ ℙ(SetID)→BOOL"/>
<org.eventb.core.axiom name="axm14" org.eventb.core.comment="If a is empty, then it is unique." org.eventb.core.label="isUnique_F1" org.eventb.core.predicate="∀a·a∈ℙ(SetID)∧isEmpty(a)=TRUE &#10;⇒ isUnique(a)=TRUE"/>
<org.eventb.core.axiom name="^" org.eventb.core.comment="If the set of cards has two cards the same id, then it is not unique.&#10;return FALSE" org.eventb.core.label="isUnique_F2" org.eventb.core.predicate="∀a·∃x,y·a∈ℙ(SetID)∧x∈a∧y∈a∧x=y &#10;⇒isUnique(a)=FALSE"/>
